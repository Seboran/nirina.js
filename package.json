{
  "name": "transpilation-ts",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "rimraf dist && tsc-watch --sourceMap --onSuccess 'node dist/main.js'",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "yarn tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "precommit": "yarn format && yarn test && git add ."
  },
  "devDependencies": {
    "@types/prettier": "^2.7.2",
    "@vitest/coverage-c8": "^0.29.8",
    "nodemon": "^2.0.22",
    "rimraf": "^4.4.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "prettier": "^2.8.7"
  }
}
